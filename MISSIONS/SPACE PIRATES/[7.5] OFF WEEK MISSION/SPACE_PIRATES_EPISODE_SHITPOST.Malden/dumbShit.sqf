launchSkeetLow = {

	private ["_machine"];
	_machine = skeetSkeet;

	if (!(isnil "_machine")) then
	{
		private ["_disc", "_discPos"];
		_discPos = getPosASL _machine;
		_disc = "Skeet_Clay_F" createVehicle [(_discPos select 0), (_discPos select 1), (_discPos select 2)];
		_disc setPosASL ([(_discPos select 0), (_discPos select 1), (_discPos select 2)] vectorAdd [0,0,0.6]);

		private ["_vel", "_ehCode"];
		_vel = [[0, 1, 0], (direction _machine + random 4 - random 4)] call BIS_fnc_rotateVector2D;
		_disc setVelocity [(-(_vel select 0) * 9), ((_vel select 1) * 9), 10 + (random 2)];

		sleep 1;

		//Make sure the skeet flies a bit longer than normal physics would cause.
		_vel = velocity _disc;

		private ["_i"];
		_i = 0;

		private ["_timer"];
		_timer = 1000;

		while {(((position _disc) select 2) > 0.1) && (alive _disc) && _timer > 0} do
		{
			_disc setVelocity [(_vel select 0) / (1 + (_i / 10)), (_vel select 1) / (1 + (_i / 10)), (_vel select 2) / (1 + _i)];
			_i = _i + 0.1;
			sleep 0.1;
			_timer = _timer - 1;
		};
	};

	true

};

